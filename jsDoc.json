{
    "tags": {
      "allowUnknownTags": false,
      "dictionaries": ["jsdoc"]
    },
    "source": {
      "include": ["src"],
      "exclude": ["node_modules", "test"],
      "includePattern": ".+\\.js(doc|x)?$",
      "excludePattern": "(^|\\/|\\\\)_"
    },
    "plugins": [
      "plugins/markdown"
    ],
    "templates": {
      "cleverLinks": true,
      "monospaceLinks": false
    },
    "opts": {
      "encoding": "utf8",
      "destination": "./docs",
      "recurse": true,
      "readme": "./README.md",
      "tutorials": "./tutorials"
    }
  }
  